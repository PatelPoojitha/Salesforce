    <fieldUpdates>
        <fullName>Update_Opportunity_Name</fullName>
        <field>Name</field>
        <formula>Left( Account.Name + &quot; - &quot; + if(RecordType.Name = &quot;Dealer Opportunity&quot;,&quot;&quot;,RecordType.Name + &quot; - &quot;) +text(MONTH(CloseDate)) + &quot;/&quot; + text(DAY(CloseDate)) + &quot;/&quot; + text(YEAR(CloseDate)),80)</formula>
        <name>Update Opportunity Name</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Formula</operation>
        <protected>false</protected>
        <reevaluateOnChange>false</reevaluateOnChange>
