            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <criteriaItems>
            <field>Opportunity.Closing_Probability__c</field>
            <operation>notEqual</operation>
            <value>Sold</value>
        </criteriaItems>
        <criteriaItems>
            <field>Opportunity.StageName</field>
            <operation>equals</operation>
            <value>Closed-Completed</value>
        </criteriaItems>
        <criteriaItems>
            <field>Opportunity.RecordTypeId</field>
            <operation>equals</operation>
            <value>Change Order/Cancellation,Tier2 Change Order/Cancellation</value>
        </criteriaItems>
        <criteriaItems>
            <field>Opportunity.Change_Order_Cancellation_Type__c</field>
            <operation>notEqual</operation>
            <value>Dealership Name Change,Downgrade,Full-Cancellation,Partial-Cancellation,Saved,Credit Given,Temporary Downgrade,Downgrade - Right-size Campaign,One-time fee,Exercised 30 Day Out Clause</value>
        </criteriaItems>
        <description>Automatically update Close Complete Opportunities with Sold Probability</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
