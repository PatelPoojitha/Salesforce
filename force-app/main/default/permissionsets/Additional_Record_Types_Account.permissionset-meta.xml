<?xml version="1.0" encoding="UTF-8"?>
<PermissionSet xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>Enables Account record types for Dealer, Non-dealer, and Partner</description>
    <hasActivationRequired>false</hasActivationRequired>
    <label>Additional Record Types Account</label>
    <recordTypeVisibilities>
        <recordType>Account.Dealer</recordType>
        <visible>true</visible>
    </recordTypeVisibilities>
    <recordTypeVisibilities>
        <recordType>Account.Non_Dealer</recordType>
        <visible>true</visible>
    </recordTypeVisibilities>
    <recordTypeVisibilities>
        <recordType>Account.Partner</recordType>
        <visible>true</visible>
    </recordTypeVisibilities>
</PermissionSet>
