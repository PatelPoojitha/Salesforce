        </rules>
    </decisions>
    <description>Updated with simplified Users</description>
    <environments>Default</environments>
    <formulas>
        <name>CloseDate15Days</name>
        <dataType>DateTime</dataType>
        <expression>NOW() + 15</expression>
    </formulas>
    <formulas>
        <name>forDueDate</name>
        <dataType>Date</dataType>
        <expression>IF(WEEKDAY(TODAY() + 1) = 1, TODAY() + 2, IF(WEEKDAY(TODAY() + 1) = 7, TODAY() + 3, TODAY() + 1))</expression>
    </formulas>
    <formulas>
        <name>forDueDate02</name>
        <dataType>Date</dataType>
        <expression>{!Loop_Batch_01.ClosedDate} + 15</expression>
    </formulas>
    <formulas>
        <name>forDueDate03</name>
        <dataType>Date</dataType>
        <expression>IF(NOT(ISBLANK({!Loop_Batch_02.Enrollment_Case__c})), {!Loop_Batch_02.Enrollment_Case__r.ClosedDate} + 15, IF(NOT(ISBLANK({!Loop_Batch_02.Enrollment_Case__c})), {!Loop_Batch_02.Enrollment_Case__r.ClosedDate} + 15, NOW() + 15))</expression>
    </formulas>
    <formulas>
        <name>forLastQuartelyReview</name>
        <dataType>Date</dataType>
        <expression>TODAY() + 60</expression>
    </formulas>
    <formulas>
        <name>forLaunchDate45days</name>
        <dataType>Date</dataType>
        <expression>TODAY() + 45</expression>
    </formulas>
    <formulas>
        <name>forLaunchDate90days</name>
        <dataType>Date</dataType>
        <expression>TODAY() + 90</expression>
    </formulas>
    <formulas>
        <name>forNow5days</name>
        <dataType>DateTime</dataType>
        <expression>NOW() + 5</expression>
    </formulas>
    <formulas>
        <name>forOwner</name>
        <dataType>String</dataType>
        <expression>IF(NOT(ISBLANK({!Loop_Batch_02.Current_Case__c})), {!Loop_Batch_02.Current_Case__r.PM_Assigned__c}, IF(NOT(ISBLANK({!Loop_Batch_02.Enrollment_Case__c})), {!Loop_Batch_02.Enrollment_Case__r.PM_Assigned__c},{!Loop_Batch_02.Service_Location__r.OwnerId}))</expression>
    </formulas>
    <formulas>
        <name>forWeekDay</name>
        <dataType>Number</dataType>
        <expression>WEEKDAY(TODAY())</expression>
        <scale>2</scale>
    </formulas>
